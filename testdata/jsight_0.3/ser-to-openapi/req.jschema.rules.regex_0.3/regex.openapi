{
    "openapi": "3.0.3",
    "info": {
      "title": "",
      "version": ""
    },
    "paths": {
      "/request/jschema/rules/regex": {
        "get": {
          "responses": {
            "200": {
              "description": "",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "string",
                    "pattern": "^(?:[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*|\"(?:[\\x01-\\x08\\x0b\\x0c\\x0e-\\x1f\\x21\\x23-\\x5b\\x5d-\\x7f]|\\\\[\\x01-\\x09\\x0b\\x0c\\x0e-\\x7f])*\")@(?:(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?|\\[(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?|[a-z0-9-]*[a-z0-9]:(?:[\\x01-\\x08\\x0b\\x0c\\x0e-\\x1f\\x21-\\x5a\\x53-\\x7f]|\\[\\x01-\\x09\\x0b\\x0c\\x0e-\\x7f])+)\\])$",
                    "example": "tom@cat.com"
                  }
                }
              }
            },
            "201": {
              "description": "",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "phone": {
                        "type": "string",
                        "pattern": "^\\+?(\\d[\\d-. ]+)?(\\([\\d-. ]+\\))?[\\d-. ]+\\d$",
                        "example": "+91 (123) 456-7890"
                      },
                      "time": {
                        "type": "string",
                        "pattern": "^(0?[1-9]|1[0-2]):[0-5]\\d\\s?(am|pm)?$",
                        "example": "01:00 pm"
                      },
                      "url": {
                        "type": "string",
                        "pattern": "^(https?:\/\/)?([\\da-z\\.-]+)\\.([a-z\\.]{2,6})([\/\\w \\.-]*)*\/?$",
                        "example": "http://www.google.com"
                      },
                      "ccNumber": {
                        "type": "string",
                        "pattern": "^(?:4[0-9]{12}(?:[0-9]{3})?|5[1-5][0-9]{14}|3[47][0-9]{13}|3(?:0[0-5]|[68][0-9])[0-9]{11}|6(?:011|5[0-9]{2})[0-9]{12}|(?:2131|1800|35\\d{3})\\d{11})$",
                        "example": "4111111111111111"
                      },
                      "utf": {
                        "type": "string",
                        "pattern": "^[А-Яа-я! ]+$",
                        "example": "Русское слово!"
                      }
                    },
                    "required": ["phone", "time", "url", "ccNumber", "utf"],
                    "additionalProperties": false
                  }
                }
              }
            },
            "202": {
              "description": "",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "pattern": "^(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$",
                      "example": "192.168.1.1"
                    }
                  }
                }
              }
            },
            "203": {
              "description": "",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "string",
                    "example": "Any string",
                    "pattern": "[A-Za-z ]+"
                  }
                }
              }
            }

          }
        }
      }
    },
    "components": {
      "schemas": {
        "catEmailUserType": {
          "type": "string",
          "example": "abc@cat.com",
          "pattern": "^[a-z]+@cat.com$"
        }
      }
    }
}